<h2>Search Results</h2>
<form action="/results" method="GET">
    <label for="name">Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% results.forEach(result => { %>
    <div><%= result.name %> </div>
<% }); %>